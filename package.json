{
  "name": "express-bugblaster",
  "version": "0.0.1",
  "description": "A Express Js error debugging assistant with code inspection.",
  "main": "src",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node src",
    "test": "jest --coverage"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "chalk": "^4.1.2",
    "express": "^4.18.2",
    "stack-trace": "^0.0.10",
    "supertest": "^7.0.0"
  },
  "keywords": [
    "node",
    "express",
    "debugging",
    "errors"
  ],
  "author": "Salman Zafar <salmanzafar949@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/salmanzafar949/express-bug-blaster"
  },
  "homepage": "https://github.com/salmanzafar949/express-bug-blaster#readme",
  "bugs": {
    "url": "https://github.com/salmanzafar949/express-bug-blaster/issues",
    "email": "salmanzafar949@gmail.com"
  },
  "funding": "https://github.com/salmanzafar949/express-bug-blaster?sponsor=1",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  }
}
